{
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./../.vscode/typings",
      "./src/types"
    ],
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6"],
    "sourceMap": true,
    "outDir": "./out",
    "rootDir": "./src",
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  }
}
